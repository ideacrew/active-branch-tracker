{
  "name": "idc",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "ng:update": "cross-env FORCE_NG_UPDATE=true ng update",
    "seed:db": "cross-env FIRESTORE_EMULATOR_HOST=localhost:8080 node db/scripts/seedDb",
    "start:live": "ng serve dashboard",
    "start:codespaces": "ng serve dashboard --publicHost=${CODESPACE_NAME}-4200.${GITHUB_CODESPACES_PORT_FORWARDING_DOMAIN}:443",
    "start:emulators": "firebase emulators:start --project=demo-project --only=firestore,functions,auth --import=./db/exports --export-on-exit=./db/exports",
    "start:emulators:testing": "firebase emulators:start --project=demo-project --only=firestore,functions,auth",
    "start:functions": "cd functions && npm run watch",
    "start:app": "ng serve dashboard --configuration=emulator --publicHost=${CODESPACE_NAME}-4200.${GITHUB_CODESPACES_PORT_FORWARDING_DOMAIN}:443",
    "test": "nx run-many --target=test --all=true",
    "lint": "nx run-many --target=lint --all=true",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "build": "ng build dashboard --configuration=production",
    "deploy:all": "npx firebase deploy",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:dashboard": "firebase deploy --only hosting",
    "workspace-generator": "nx workspace-generator"
  },
  "private": true,
