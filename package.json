  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "seed:db": "cross-env FIRESTORE_EMULATOR_HOST=localhost:8080 node db/scripts/seedDb",
    "start:live": "ng serve",
    "start": "concurrently \"ng serve --configuration=emulator\" \"cd functions && npm run watch\"",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
