{
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tslint.json": "*",
    "nx.json": "*",
    ".eslintrc.json": "*",
    ".github/workflows/test-and-deploy.yml": "*"
  },
  "affected": {
    "defaultBase": "trunk"
  },
  "npmScope": "idc",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "auth": {
      "tags": []
    },
    "branches-data-access": {
      "tags": ["scope:dashboard", "type:data-access"]
    },
    "branches-feature": {
      "tags": ["scope:dashboard", "type:feature"]
    },
    "branches-ui-components": {
      "tags": ["scope:dashboard", "type:ui"]
    },
    "cucumbers": {
      "tags": []
    },
    "dashboard": {
      "tags": []
    },
    "dashboard-e2e": {
      "tags": [],
      "implicitDependencies": ["dashboard"]
    },
    "display-config": {
      "tags": ["scope:dashboard", "type:util"]
    },
    "environments-data-access": {
      "tags": []
    },
    "environments-feature": {
      "tags": []
    },
    "pull-requests-data-access": {
      "tags": []
    },
    "pull-requests-feature": {
      "tags": []
    },
    "rspecs": {
      "tags": []
    },
    "testing": {
      "tags": []
    },
    "user-data-access": {
      "tags": []
    },
    "util": {
      "tags": []
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  }
}
